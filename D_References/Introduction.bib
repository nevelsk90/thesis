	_comment = {Another tol2 paper, this time with inducible GFP expression.}
}


@article{petzold2013transcriptome,
	title = {The transcript catalogue of the short-lived fish {Nothobranchius} furzeriprovides insights into age-dependent changes of {mRNA} levels},
