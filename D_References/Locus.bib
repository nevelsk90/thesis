	pages = {1--19},
	file = {Full Text PDF:/home/will/.zotero/zotero/trvnmvj8.default/zotero/storage/PCBBWXDX/Patel et al. - 2018 - Diversity of Immunoglobulin (Ig) Isotypes and the .pdf:application/pdf;Snapshot:/home/will/.zotero/zotero/trvnmvj8.default/zotero/storage/JUFNYMBX/10.html:text/html}
}

@article{magor2015affinity,
