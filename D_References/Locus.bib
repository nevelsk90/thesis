	pmid = {23884653},
	keywords = {Read, toProcess},
	pages = {13097--13102},
	file = {Full Text PDF:/home/will/.zotero/zotero/trvnmvj8.default/zotero/storage/CYWKURTR/Xu et al. - 2013 - Teleost skin, an ancient mucosal surface that elic.pdf:application/pdf;Snapshot:/home/will/.zotero/zotero/trvnmvj8.default/zotero/storage/SFT3J6D4/13097.html:text/html}
}


