@article{ehrenmann2011domaingapalign,
	title = {{IMGT}/{DomainGapAlign}: {IMGT} {Standardized} {Analysis} of {Amino} {Acid} {Sequences} of {Variable}, {Constant}, and {Groove} {Domains} ({IG}, {TR}, {MH}, {IgSF}, {MhSF})},
